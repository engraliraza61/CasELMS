@{
    ViewData["Title"] = "RegisterPage";
    Layout = "~/Views/Shared/elmsLayout.cshtml";
}
<script src="~/js/student.js"></script>
<div class="full-page login-page" filter-color="black" data-image="../../assets/img/login.jpg">
    <div class="content">
        <div class="container">
            <div class="row">
                <div class="">
                    <div class="card card-login card-hidden" align="center">
                        <div class="card-header text-center" data-background-color="rose">
                            <h4 class="card-title">Register</h4>
                        </div>
                        <p class="category text-center">
                            Or Be Classical
                        </p>
                        <div class="card-content">
                            <div class="row col-md-12">
                                <div class="col-md-4">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <i class="material-icons">face</i>
                                        </span>

                                        <div class="form-group label-floating">
                                            <label class="control-label">FirstName</label>
                                            <input type="text" id="studentFirstName" class="form-control">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <i class="material-icons">face</i>
                                        </span>

                                        <div class="form-group label-floating">
                                            <label class="control-label">LastName</label>
                                            <input type="text" id="studentLastName" class="form-control">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <i class="material-icons">UserName</i>
                                        </span>

                                        <div class="form-group label-floating">
                                            <label class="control-label">UserName</label>
                                            <input type="text" id="studentUserName" class="form-control">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row col-md-12">
                                <div class="col-md-4">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <i class="material-icons">email</i>
                                        </span>

                                        <div class="form-group label-floating">
                                            <label class="control-label">Email</label>
                                            <input type="text" id="studnetEmail" class="form-control">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <i class="material-icons">password</i>
                                        </span>

                                        <div class="form-group label-floating">
                                            <label class="control-label">Password</label>
                                            <input type="text" id="studentPassword" class="form-control">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <i class="material-icons">phone</i>
                                        </span>
                                        <div class="form-group label-floating">
                                            <label class="control-label">PhoneNo</label>
                                            <input type="text" id="studnetPhoneNo" class="form-control">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row col-md-12">
                                <div class="col-md-4">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <i class="material-icons">address</i>
                                        </span>

                                        <div class="form-group label-floating">
                                            <label class="control-label">Address</label>
                                            <input type="text" id="studentAddress" class="form-control">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <i class="material-icons">email</i>
                                        </span>

                                        <div class="form-group label-floating">
                                            <label class="control-label">PostalCode</label>
                                            <input type="text" id="studentPostalCode" class="form-control">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="footer text-center">
                            <button type="submit" id="studentRegisterButton" class="btn btn-rose btn-simple btn-wd btn-lg">Let's go</button>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <footer class="footer">
            <div class="container">
                <nav class="pull-left">
                    <ul>
                        <li>
                            <a href="#">
                                Home
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                Company
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                Portofolio
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                Blog
                            </a>
                        </li>
                    </ul>
                </nav>
                <p class="copyright pull-right">
                    &copy;
                    <script>document.write(new Date().getFullYear())</script> <a href="https://www.creative-tim.com/"> Creative Tim </a>,  made with love for a better web
                </p>
            </div>
        </footer>
    </div>
</div>